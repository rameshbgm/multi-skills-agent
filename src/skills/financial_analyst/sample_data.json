{
  "skill_metadata": {
    "name": "Financial Analyst",
    "version": "1.0.0",
    "category": "Finance"
  },
  "common_metrics": {
    "valuation": {
      "pe_ratio": {
        "name": "Price-to-Earnings Ratio",
        "formula": "Market Price / Earnings Per Share",
        "interpretation": {
          "low": "< 15 - potentially undervalued",
          "average": "15-25 - fairly valued",
          "high": "> 25 - potentially overvalued or high growth"
        }
      },
      "pb_ratio": {
        "name": "Price-to-Book Ratio",
        "formula": "Market Price / Book Value Per Share",
        "interpretation": {
          "low": "< 1 - trading below asset value",
          "average": "1-3 - fairly valued",
          "high": "> 3 - premium valuation"
        }
      },
      "peg_ratio": {
        "name": "PEG Ratio",
        "formula": "P/E Ratio / Earnings Growth Rate",
        "interpretation": {
          "undervalued": "< 1",
          "fair": "1",
          "overvalued": "> 1"
        }
      }
    },
    "profitability": {
      "roe": {
        "name": "Return on Equity",
        "formula": "Net Income / Shareholders Equity",
        "good_range": "15-20%+"
      },
      "roa": {
        "name": "Return on Assets",
        "formula": "Net Income / Total Assets",
        "good_range": "5%+"
      },
      "gross_margin": {
        "name": "Gross Margin",
        "formula": "(Revenue - COGS) / Revenue",
        "note": "Industry dependent"
      },
      "net_margin": {
        "name": "Net Profit Margin",
        "formula": "Net Income / Revenue",
        "good_range": "10%+"
      }
    },
    "liquidity": {
      "current_ratio": {
        "name": "Current Ratio",
        "formula": "Current Assets / Current Liabilities",
        "healthy_range": "1.5 - 3.0"
      },
      "quick_ratio": {
        "name": "Quick Ratio",
        "formula": "(Current Assets - Inventory) / Current Liabilities",
        "healthy_range": "> 1.0"
      }
    },
    "risk": {
      "beta": {
        "name": "Beta",
        "description": "Volatility relative to market",
        "interpretation": {
          "low": "< 1 - less volatile than market",
          "market": "1 - moves with market",
          "high": "> 1 - more volatile than market"
        }
      },
      "sharpe_ratio": {
        "name": "Sharpe Ratio",
        "formula": "(Return - Risk-Free Rate) / Standard Deviation",
        "interpretation": {
          "poor": "< 1",
          "good": "1-2",
          "excellent": "> 2"
        }
      }
    }
  },
